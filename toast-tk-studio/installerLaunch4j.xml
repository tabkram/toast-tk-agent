<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar></jar>
  <outfile>install\Toast Studio.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>toast-tk-automation-client\src\main\resources\toast.ico</icon>
  <classPath>
    <mainClass>com.synaptix.toast.swing.agent.AgentBoot</mainClass>
    <cp>lib\*</cp>
  </classPath>
  <jre>
    <!-- <path>C:\Program Files\Java\jre1.8.0_66</path> -->
    <bundledJre64Bit>false</bundledJre64Bit>
    <bundledJreAsFallback>false</bundledJreAsFallback>
    <minVersion>1.8.0_60</minVersion>
    <maxVersion>1.8.0_66</maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64/32</runtimeBits>
    <opt>-Dtoast.home="%TOAST_HOME%"</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0</txtFileVersion>
    <fileDescription>Toast Tk Studio</fileDescription>
    <copyright>Copyright (c) 2015-2016 Synaptix Labs</copyright>
    <productVersion>1.0.0.0</productVersion>
    <txtProductVersion>1.0</txtProductVersion>
    <productName>Toast Tk Studio</productName>
    <companyName>Synaptix-Labs</companyName>
    <internalName>Toast Tk Studio</internalName>
    <originalFilename>Toast Tk - Studio.exe</originalFilename>
  </versionInfo>
</launch4jConfig>