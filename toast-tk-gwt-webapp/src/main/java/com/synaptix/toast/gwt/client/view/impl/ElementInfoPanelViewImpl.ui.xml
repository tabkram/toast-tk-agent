<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HTMLPanel>
				<span>Name: </span>
				<g:TextBox ui:field="nameField" width="200px">name</g:TextBox>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<span>Element Type: </span>
				<g:TextBox ui:field="typeField" width="200px">type</g:TextBox>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<span>Location: </span>
				<g:TextBox ui:field="locationField" width="200px">location</g:TextBox>
				<g:Button ui:field="fecthLocationButton">T</g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<span>Method: </span>
				<g:TextBox ui:field="methodField" width="200px">method</g:TextBox>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<span>Position: </span>
				<g:TextBox ui:field="positionField" width="200px">position</g:TextBox>
			</g:HTMLPanel>
			<g:Button ui:field="saveButton">Save</g:Button>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 