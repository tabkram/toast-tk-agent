<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.right {
			float: right;
		}
	</ui:style>
	<g:HTMLPanel>
		<div>
			<g:Button styleName="{style.right}" ui:field="refreshButton">Refresh</g:Button>
			<h2>Domain Setup</h2>
		</div>
		<div class="row-fluid">
			<div class="span3">
				<g:SimplePanel ui:field="pageList" />
			</div>
			<div class="span4">
				<g:SimplePanel ui:field="pageElementList" />
			</div>
			<div class="span4">
				<g:SimplePanel ui:field="elementInfoPanel" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 